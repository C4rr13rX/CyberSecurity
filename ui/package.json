{
  "name": "paranoid-av-ui",
  "version": "0.1.0",
  "private": true,
  "description": "Ionic Angular desktop shell for the Paranoid antivirus suite",
  "main": "electron/main.js",
  "scripts": {
    "ng:serve": "ng serve",
    "ng:build": "ng build",
    "ng:lint": "ng lint",
    "ng:test": "ng test",
    "electron:start": "wait-on http://localhost:4200 && electron ./electron/main.js",
    "electron:build": "npm run ng:build && electron ./electron/main.js",
    "start": "npm-run-all -p ng:serve electron:start",
    "build": "npm run electron:build"
  },
  "dependencies": {
